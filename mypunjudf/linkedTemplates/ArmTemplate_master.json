{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mypunjudf"
		},
		"ls_mysablob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_mysablob'"
		},
		"ls_order_items_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_order_items_dl'"
		},
		"ls_s3_order_items_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'ls_s3_order_items'"
		},
		"ls_sql_retaildb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_retaildb'"
		},
		"ls_order_items_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mysadls.dfs.core.windows.net/"
		},
		"ls_s3_order_items_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAYWBJYCPVPCYWPOJ7"
		},
		"ls_sql_retaildb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myserver98.database.windows.net"
		},
		"ls_sql_retaildb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "RETAILDB"
		},
		"ls_sql_retaildb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "arun"
		},
		"tr_ingest_process_orders_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4c2bb014-39a9-4bee-8893-a4f61d6d85ed/resourceGroups/Customer360/providers/Microsoft.Storage/storageAccounts/mysablob"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mypunjudf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_mysablob_connectionString": {
						"value": "[parameters('ls_mysablob_connectionString')]"
					},
					"ls_order_items_dl_accountKey": {
						"value": "[parameters('ls_order_items_dl_accountKey')]"
					},
					"ls_s3_order_items_secretAccessKey": {
						"value": "[parameters('ls_s3_order_items_secretAccessKey')]"
					},
					"ls_sql_retaildb_password": {
						"value": "[parameters('ls_sql_retaildb_password')]"
					},
					"ls_order_items_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_order_items_dl_properties_typeProperties_url')]"
					},
					"ls_s3_order_items_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_s3_order_items_properties_typeProperties_accessKeyId')]"
					},
					"ls_sql_retaildb_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_retaildb_properties_typeProperties_server')]"
					},
					"ls_sql_retaildb_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_retaildb_properties_typeProperties_database')]"
					},
					"ls_sql_retaildb_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_retaildb_properties_typeProperties_userName')]"
					},
					"tr_ingest_process_orders_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_process_orders_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}